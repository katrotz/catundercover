{
  "name": "catundercover",
  "version": "0.1.0",
  "description": "Catundercover portfolio application",
  "main": "src/app/index.js",
  "bin": {},
  "scripts": {
    "postinstall": "jspm install",
    "reinstall": "rm -rf node_modules && rm -rf src/vendor && npm i",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "jspm bundle app/index.js src/dist/catundercover.js -i",
    "start": "./bin/server"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:katrotz/catundercover.git"
  },
  "keywords": [
    "Angular2",
    "Frontend",
    "catundercover",
    "portfolio"
  ],
  "owner": [
    "Vyacheslav Cotruta <katrotz@gmail.com> (http://katrotz.space)"
  ],
  "license": "MIT",
  "devDependencies": {
    "jspm": "^0.17.0-beta.22",
    "live-server": "^1.0.0"
  },
  "dependencies": {},
  "jspm": {
    "name": "catundercover",
    "main": "index.js",
    "directories": {
      "baseURL": "src",
      "packages": "src/vendor"
    },
    "dependencies": {
      "@angular/http": "npm:@angular/http@^2.0.0-rc.5",
      "@angular/router": "npm:@angular/router@^3.0.0-rc.1",
      "@angular2-material/button": "npm:@angular2-material/button@^2.0.0-alpha.5-2",
      "@angular2-material/card": "npm:@angular2-material/card@^2.0.0-alpha.5-2",
